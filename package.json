{
  "name": "regl-map-animation",
  "version": "1.1.2",
  "description": "Configurable animation showing a transition from categorized geographic points on a map into a chart",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel index.js",
    "build": "parcel build index.js --global mapAnimation --no-source-maps --out-file bundle.js",
    "build:umd": "parcel build index.js --global mapAnimation --no-source-maps --out-file bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eurostat/regl-map-animation.git"
  },
  "files": [
    "./dist"
  ],
  "keywords": [
    "regl",
    "map",
    "eurostat",
    "animation",
    "webgl"
  ],
  "author": "Joe Davies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eurostat/regl-map-animation/issues"
  },
  "homepage": "https://github.com/eurostat/regl-map-animation#readme",
  "dependencies": {
    "d3": "^5.15.0",
    "regl": "^1.3.13"
  },
  "devDependencies": {
    "parcel": "^1.12.4"
  }
}
